--particles

function make_splash(x, y, dx, dy, dz, col)
    for i = 1, 2 do
        add(
            particles, {
                x = x, y = y,
                z = 0, g = 1,
                dx = dx + (rnd() - 0.5) * 0.5,
                dy = dy + (rnd() - 0.5) * 0.5,
                dz = dz + (rnd() - 0.5) * 0.5,
                life = 20,
                col = col
            }
        )
    end
end

function update_particle(p)
    p.z += p.dz
    p.dz -= 0.05 * p.g
    p.y += p.dy
    p.x += p.dx
    p.life -= 1
    if p.z < 0 or p.life <= 0 then
        del(particles, p)
    end
end

function draw_particles()
    for p in all(particles) do
        update_particle(p)
        pset(p.x, p.y - p.z, p.col)
    end
end

