--particles

function make_splash(x,y,dx,dy,dz)

	for i=1,4 do
		add(particles, {
		 x=x, y=y,
		 z=0, g=1,
		 dx=dx+(rnd()-0.5)*0.5,
		 dy=dy+(rnd()-0.5)*0.5,
		 dz=dz+(rnd()-0.5)*0.5,
		 life=20,
		 col=7
		})
	end
end

function update_particle(p)
    p.z+=p.dz
    p.dz-=0.05*p.g
    p.y+=p.dy
    p.x+=p.dx
    p.life-=1
    if p.z<0 or p.life<=0 then
     del(particles,p)
    end
   end

-->8
--player

function move_player(p)
    -- acceleration based on input
    if (btn(‚¨ÖÔ∏è)) then
        p.dx -= p.spd
    elseif (btn(‚û°Ô∏è)) then
        p.dx += p.spd
    else
        p.dx *= 0.8  -- deceleration when no input
    end
    
    if (btn(‚¨ÜÔ∏è)) then
        p.dy -= p.spd
    elseif (btn(‚¨áÔ∏è)) then
        p.dy += p.spd
    else
        p.dy *= 0.8  -- deceleration when no input
    end
    
    -- jump when O is pressed and we're on the water
    if (btn(üÖæÔ∏è) and p.z <= 0) then
        p.dz = p.jump_power
    end
    
    -- apply gravity and update z position
    p.dz -= p.g
    p.z += p.dz
    
    -- bounce off water with 30% of velocity maintained
    if p.z < 0 then
        p.z = 0
        if p.dz < -1 then  -- only splash if we hit with some speed
            for i=1,25 do
                make_splash(p.x + ((i/25)*8), p.y+8, p.dx*(rnd()-0.5), p.dy*(rnd()-0.5), 0.4)
            end
        end
        p.dz = 0
    end
    
    -- update position
    p.x += p.dx
    p.y += p.dy
    
    -- keep in bounds (optional)
    p.x = mid(0, p.x, 208)
    p.y = mid(0, p.y, 208)
end

